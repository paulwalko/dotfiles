"amixer -c 1 set Master 5%+"
	XF86AudioRaiseVolume

"amixer -c 1 set Master 5%-"
	XF86AudioLowerVolume

"amixer -c 1 set Master toggle && amixer -c 1 set Speaker unmute"
	XF86AudioMute

"amixer -c 1 set Capture toggle"
	XF86AudioMicMute

"light -A 5"
	XF86MonBrightnessUp

"light -U 5"
	XF86MonBrightnessDown

"light -S 1"
	XF86Tools

"light -S 50"
	XF86Search

"light -S 100"
	XF86LaunchA

