"amixer -c 1 set Master 5%+"
	XF86AudioRaiseVolume

"amixer -c 1 set Master 5%-"
	XF86AudioLowerVolume

"amixer -c 1 set Master toggle && amixer -c 1 set Speaker unmute"
	XF86AudioMute

"amixer -c 1 set Capture toggle"
	XF86AudioMicMute

"xbacklight -inc 5"
	XF86MonBrightnessUp

"xbacklight -dec 5"
	XF86MonBrightnessDown

"xbacklight -set 1"
	XF86Tools

"xbacklight -set 40"
	XF86Search

"xbacklight -set 75"
	XF86LaunchA

"xbacklight -set 100"
	XF86Explorer

"i3lock"
	Super_L
